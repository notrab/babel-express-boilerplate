{
  "name": "express-babel-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Designed for express backend apps. Write ES2015, Deploy ES5. Babel-backed.",
  "scripts": {
    "start": "heroku local -f Procfile.local",
    "start:prod": "node dist/server",
    "build": "npm run build-dist",
    "build-dist": "rimraf dist && babel ./src -d dist --copy-files",
    "heroku-postbuild": "npm run build"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "keywords": [
    "boilerplate",
    "es2015",
    "es6",
    "expressjs"
  ],
  "author": "Jamie Barton <jamie@launchmade.com> (http://launchmade.com/)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "rimraf": "^2.5.3"
  },
  "devDependencies": {
    "babel-register": "^6.9.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.9.2"
  }
}
